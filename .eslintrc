{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
        "jsx": true,
        "modules": true,
        "experimentalObjectRestSpread": true
    }
  },
  "extends": ["airbnb"],
  "rules": {
    "radix": 0,
    "es-beautifier/multiline-array-elements": 0,
    "no-unused-expressions": 0,
    "react/no-unescaped-entities": 0,
    "react/jsx-filename-extension": 0,
    "react/jsx-curly-spacing": 0,
    "import/no-extraneous-dependencies": 0,
    "global-require": 0,
    "no-trailing-spaces": ["error", { "skipBlankLines": true }],
    "max-len": ["error", 121],
    "arrow-parens": 0,
    "template-curly-spacing": 0,
    "no-confusing-arrow": 0,
    "es-beautifier/jsx-multiline-elements": 0,
    "import/prefer-default-export": 0,
    "react/forbid-prop-types": 0,
    "class-methods-use-this": 0,
    "es-beautifier/jsx-separateline-literals": 0,
    "es-beautifier/jsx-multiline-attributes": 0,
    "es-beautifier/multiline-import-specifiers": 0,
    "react/jsx-first-prop-new-line": 0,
    "react/prefer-stateless-function": 0,
    "es-beautifier/multiline-object-properties": 0
  }
}